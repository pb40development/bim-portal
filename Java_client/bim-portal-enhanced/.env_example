# BIM Portal API Credentials
# Copy this file to .env and fill in your credentials

# Required credentials
BIM_PORTAL_USERNAME=your_username_here
BIM_PORTAL_PASSWORD=your_password_here

# Optional configuration
#BIM_PORTAL_BASE_URL=https://via.bund.de/bim # PRDO
BIM_PORTAL_BASE_URL=https://via.bund.de/bmdv/bim-portal/edu/bim #EDU
EXPORT_DIRECTORY=exports
REQUEST_TIMEOUT=30
MAX_RETRIES=3
LOG_LEVEL=INFO

# Advanced configuration
MAX_PROJECTS_TO_TEST=10
VERIFY_SSL=true